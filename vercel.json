{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET", "POST"]
        }
    ],
    "functions": {
        "api": {
            "runtime": "nodejs14.x",
            "maxDuration": 30
        }
    }
}